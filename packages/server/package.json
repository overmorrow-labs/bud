{
  "name": "bud-server",
  "version": "0.0.1",
  "description": "A robust Fastify server with TypeScript, ready for development and npm packaging.",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prepack": "npm run build"
  },
  "keywords": [
    "fastify",
    "typescript",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/sensible": "^6.0.3",
    "fastify": "^5.3.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist"
  ]
}
